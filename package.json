{
  "name": "simple_web_boilerplate",
  "version": "1.0.0",
  "description": "simple_web_boilerplate",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./dist",
    "compile_less": "node scripts/compile_less.js",
    "watch_less": "onchange -f 'change'  -v -i -k  'src/less/**/*.less' -- node scripts/compile_less.js",
    "watch_js": "onchange -f 'change'  -v -i -k  'src/less/**/*.less' -- node scripts/compile_less.js",
    "http_server": "http-server -p 63345 ./dist",
    "browser_sync": "browser-sync start --server --files 'dist/**' --single --port 63345",
    "debug": "npm-run-all --parallel http_server watch_less",
    "copy_statict_folder": "node scripts/copy_statict_folder.js",
    "build": "npm run clean && rollup -c ./build/rollup.conf.pro.js && npm run compile_less && npm run  copy_statict_folder",
    "dev": "npm run clean && rollup -c ./build/rollup.conf.dev.js && npm run compile_less && npm run  copy_statict_folder && npm run debug"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "glob": "^7.1.3",
    "http-server": "^0.11.1",
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-copy-assets": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-plugin-vue": "^4.7.1",
    "rollup-plugin-terser": "^4.0.4",
    "shelljs": "^0.8.3",
    "stylelint": "^9.10.1",
    "vue-template-compiler": "^2.6.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    
    "vue": "^2.6.6"
  }
}
