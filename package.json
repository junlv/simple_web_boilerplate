{
  "name": "simple_web_boilerplate",
  "version": "1.0.0",
  "description": "simple_web_boilerplate",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./dist",
    "compile_less": "node scripts/compile_less.js",
    "watch_less": "onchange -f 'change'  -v -i -k  'src/less/**/*.less'   -- node scripts/compile_less.js",
    "watch_js": "onchange -f 'change'  -v -i -k  'src/**/*.js' --  node scripts/compile_js.js",
    "browser_sync": "browser-sync start --reload-delay 3000 --server 'dist' --files 'dist/static/css/main.css, dist/static/app/*.js'  --startPath 'views/index.html' --port 63347",
    "debug": "npm-run-all --parallel browser_sync watch_less watch_js",
    "copy_statict_folder": "node scripts/copy_statict_folder.js",
    "build": "npm run clean && rollup -c ./build/rollup.conf.pro.js && npm run compile_less && npm run  copy_statict_folder",
    "pro": "npm run fix:styles &&  npm run build ",
    "dev": "npm run clean && npm run compile_less && npm run copy_statict_folder && npm run debug",
    "dev2": "npm run clean && rollup -c ./build/rollup.conf.dev.js && npm run compile_less && npm run  copy_statict_folder && npm run debug",

    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix:styles": "prettier-stylelint --write 'src/less/**/*.less' "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^6.26.3",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "css-declaration-sorter": "^4.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "glob": "^7.1.3",
    "http-server": "^0.11.1",
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0",
    "livereload": "^0.7.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "postcss-less": "^3.1.4",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-copy-assets": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-plugin-vue": "^4.7.1",
    "shelljs": "^0.8.3",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-order": "^2.1.0",
    "stylelint-prettier": "^1.0.6",
    "vue-template-compiler": "^2.6.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "autolesshint": "^0.2.2",
    "postcss-sorting": "^4.1.0",
    "stylelint-config-idiomatic-order": "^6.2.0",
    "stylelint-config-standard": "^18.2.0",
    "superstruct": "^0.6.1",
    "vant": "^1.6.12",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "whatwg-fetch": "^3.0.0"
  }
}